@PACKAGE_INIT@

set(CATKIN_VERSION "@PROJECT_VERSION@")

include("${CMAKE_CURRENT_LIST_DIR}/all.cmake")

set(quiet)
if(${CMAKE_FIND_PACKAGE_NAME}_FIND_QUIETLY)
    set(quiet QUIET)
endif()
foreach(dep IN LISTS ${CMAKE_FIND_PACKAGE_NAME}_FIND_COMPONENTS)
    if(${CMAKE_FIND_PACKAGE_NAME}_FIND_REQUIRED_${dep})
        find_package(${dep} ${quiet} REQUIRED)
    else()
        find_package(${dep} ${quiet})
    endif()
    if(NOT ${dep}_CATKIN_PACKAGE)
        message(AUTHOR_WARNING "'${dep}' is not a catkin package. You should use 'find_package(${dep})'")
    endif()
endforeach()
