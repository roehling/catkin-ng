find_package(@target@ @target_version@ @is_required@ @is_quiet@ @extra_args@)
if(@target@_FOUND_CATKIN_PROJECT)
    add_library(catkin::@target@ INTERFACE IMPORTED)
    set_target_properties(catkin::@target@ PROPERTIES
        INTERFACE_INCLUDE_DIRECTORIES "${@target@_INCLUDE_DIRS}"
        INTERFACE_LINK_LIBRARIES "${@target@_LIBRARIES}"
    )
else()
    if(@target@_FOUND AND NOT "@is_required@" STREQUAL "")
        message(FATAL_ERROR "package '@target@' was found, but it is not a catkin package")
    endif()
endif()
